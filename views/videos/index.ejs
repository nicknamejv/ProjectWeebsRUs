<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Weebs R Us</title>
</head>
<body>

    <h1 class='index_title'>
        Weebs R Us
    </h1>

    <p id="video">
        <% videos.forEach(allVideo => { %>
            <ul class="video_list">
                <li>
                    <b>Title:</b>
                    <%= allVideo.title %>
                </li>
                <li>
                    <b>Image:</b> <%= allVideo.videoImage %>
                </li>
                <li>
                    <b>Description:</b> <%= allVideo.description %>
                </li>
                <li>
                    <b>Genre:</b> <%= allVideo.genre %>
                </li>
                <li>
                    <iframe width="420" height="315"
                    src="<%= allVideo.videoLink %>">
                    </iframe>
                </li>
                <li>
                    <b>Recommended?</b> <%= allVideo.isRecommended %>
                </li>
            </ul>
        <% }); %> 
    </p>


</body>
</html>